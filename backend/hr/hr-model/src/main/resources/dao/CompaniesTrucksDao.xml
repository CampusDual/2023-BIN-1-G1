<?xml version="1.0" encoding="UTF-8"?>
<JdbcEntitySetup
        xmlns="http://www.ontimize.com/schema/jdbc"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.ontimize.com/schema/jdbc http://www.ontimize.com/schema/jdbc/ontimize-jdbc-dao.xsd"
        catalog="" schema="${mainschema}" table="companies_plates"
        datasource="mainDataSource" sqlhandler="dbSQLStatementHandler">
    <DeleteKeys>
        <Column>id_companies_plates</Column>
    </DeleteKeys>
    <UpdateKeys>
        <Column>id_companies_plates</Column>
    </UpdateKeys>
    <GeneratedKey>id_companies_plates</GeneratedKey>
    <Queries>
        <Query id="default">
            <Sentence>
                <![CDATA[
                SELECT
                    #COLUMNS#
                FROM companies_plates
                #WHERE#
                ]]>
            </Sentence>
        </Query>
        <Query id="get_not_owned_plates">
            <![CDATA[
               SELECT
                    plate_number
               FROM plates as p
               INNER JOIN companies_plates as cp on cp.plate_id = p.plate_id
               WHERE cp.id_company <>
            ]]>
        </Query>
    </Queries>
</JdbcEntitySetup>